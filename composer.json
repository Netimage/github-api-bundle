{
  "name": "maxikg/github-api-bundle",
  "description": "A bridge for using knplabs/github-api in Symfony 2/3.",
  "minimum-stability": "stable",
  "keywords": ["github", "api client", "symfony 3"],
  "license": "MIT",
  "authors": [
    {
      "name": "Max Walsch",
      "email": "max.walsch@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/http-kernel": "~2.7|~3.0",
    "symfony/dependency-injection": "~2.7|~3.0",
    "symfony/config": "~2.7|~3.0",
    "knplabs/github-api": "~1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-0": {
      "Maxikg\\GithubApiBundle": ""
    }
  },
  "target-dir": "Maxikg/GithubApiBundle"
}
